{
  "presets": [
    [
      "@babel/preset-env",
      {
        "forceAllTransforms": true,
        "targets": {
          "browsers": [
            "last 2 versions",
            "edge >= 38",
            "safari >= 9",
            "ie >= 11",
            "chrome >= 60",
            "firefox >= 51"
          ]
        },
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-object-rest-spread"],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
